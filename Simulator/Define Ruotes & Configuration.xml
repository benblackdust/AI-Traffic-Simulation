<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <vType id="car" accel="2.0" decel="4.5" sigma="0.5" length="5" maxSpeed="25" color="1,0,0"/>

    <flow id="north_flow" type="car" begin="0" end="3600" number="500" from="north_in" to="south_out"/>
    <flow id="south_flow" type="car" begin="0" end="3600" number="500" from="south_in" to="north_out"/>
    <flow id="east_flow" type="car" begin="0" end="3600" number="500" from="east_in" to="west_out"/>
    <flow id="west_flow" type="car" begin="0" end="3600" number="500" from="west_in" to="east_out"/>
</routes>

<configuration>
    <input>
        <net-file value="memwork.net.xml"/>
        <route-files value="meme.rou.xml"/>
    </input>
    <time>
        <begin value="0"/>
        <end value="3600"/>
    </time>
</configuration>

